{
  "name": "fuhgeddaboudit-site",
  "version": "1.0.0",
  "description": "Fuhgeddaboudit is a chrome extension to clear your head from AI related jargon",
  "source": "src/index.html",
  "scripts": {
    "start": "npm run clean && parcel",
    "build": "npm run clean && parcel build",
    "deploy": "npm run build && gh-pages -d dist -b gh-pages",
    "clean": "rimraf dist",
    "prepare": "husky install"
  },
  "targets": {
    "default": {
      "publicUrl": "./"
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desaianand1/Fuhgeddaboudit-site.git"
  },
  "author": "Anand Desai",
  "license": "UNLICENCED",
  "private": true,
  "bugs": {
    "url": "https://github.com/desaianand1/Fuhgeddaboudit-site/issues"
  },
  "homepage": "https://github.com/desaianand1/Fuhgeddaboudit-site#readme",
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.8.3",
    "@parcel/transformer-webmanifest": "^2.8.3",
    "@types/node": "^18.15.12",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "parcel": "^2.8.3",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.{ts,js,css,md,html}": "prettier --write"
  }
}
